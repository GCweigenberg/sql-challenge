departments
-------
dept_number PK VARCHAR 
dept_name VARCHAR


dept_emp
-------
emp_number INT FK >- employees.emp_number
dept_number VARCHAR FK >- departments.dept_number



dept_manager
-------
dept_number VARCHAR FK >- departments.dept_number
emp_number INT FK >- employees.emp_number

employees
-------
emp_number PK INT
birth_date DATE 
first_name VARCHAR
last_name VARCHAR
sex VARCHAR
hire_date DATE 

salaries
-------
emp_number INT FK >- employees.emp_number
salary BIGINT


titles
-------
title VARCHAR
title_id VARCHAR
